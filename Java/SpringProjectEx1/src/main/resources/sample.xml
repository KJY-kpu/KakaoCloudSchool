<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="sample1" class="ioc02.Sample1">
		<constructor-arg><value>그냥 문자열</value></constructor-arg>
	</bean>
	<bean id="sample2" class="ioc02.Sample2">
		<constructor-arg><value>그냥 문자열</value></constructor-arg>
		<constructor-arg><value>1234</value></constructor-arg>
	</bean>
	<bean id="sample3" class="ioc02.Sample3">
		<constructor-arg type="int" value="7500000" />
		<constructor-arg type="java.lang.String" value="42" />
	</bean>
	<bean id="sample4" class="ioc02.Sample4">
		<constructor-arg index="1" value="7500000" />
		<constructor-arg index="0" value="42" />
	</bean>
</beans>